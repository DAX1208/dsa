#include <iostream>
#include <stack>
using namespace std;

int main() {
    int n;
    cin >> n;

    int adj[100][100];   // adjacency matrix (max 100 vertices)
    bool visited[100] = {false};

    // read adjacency matrix
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> adj[i][j];
        }
    }

    stack<int> st;
    st.push(0);
    visited[0] = true;

    while (!st.empty()) {
        int node = st.top();
        st.pop();

        cout << node << " ";

        // push neighbors in reverse order
        for (int j = n - 1; j >= 0; j--) {
            if (adj[node][j] == 1 && !visited[j]) {
                st.push(j);
                visited[j] = true;
            }
        }
    }

    return 0;
}
